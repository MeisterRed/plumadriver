{
  "name": "plumadriver",
  "version": "0.8.0",
  "description": "",
  "main": "./build/index.js",
  "dependencies": {
    "async-mutex": "^0.1.3",
    "body-parser": "^1.19.0",
    "encodeurl": "^1.0.2",
    "express": "^4.17.1",
    "express-winston": "^3.4.0",
    "express_logger": "0.0.4",
    "jsdom": "^16.1.0",
    "mime-types": "^2.1.24",
    "minimist": "^1.2.0",
    "os": "^0.1.1",
    "pjson": "^1.0.9",
    "request": "^2.88.0",
    "selenium-webdriver": "^4.0.0-alpha.1",
    "uuid": "^3.3.2",
    "validator": "^10.11.0",
    "validator-js": "^0.2.1",
    "vm2": "^3.8.4",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@typescript-eslint/eslint-plugin": "^2.18.0",
    "@typescript-eslint/parser": "^2.18.0",
    "commitlint": "^8.3.5",
    "cz-conventional-changelog": "^3.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "fs": "0.0.1-security",
    "http-post": "^0.1.1",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "jsdoc": "^3.6.2",
    "lint-staged": "^9.5.0",
    "nock": "^11.7.2",
    "nodemon": "^1.19.4",
    "pkg": "^4.4.0",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "typedoc": "^0.15.8",
    "typescript": "^3.7.5"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "<rootDir>/src/",
      "/node_modules/"
    ]
  },
  "scripts": {
    "pretest": "npm run lint && npm run compile",
    "test": "jest",
    "start": "node ./build/index.js",
    "compile": "tsc",
    "build:linux": "tsc && pkg . --target latest-linux-x64",
    "build:win": "tsc && pkg . --target latest-win-x64",
    "build:macos": "tsc && pkg . --target latest-mac-x64",
    "build:all": "tsc && pkg . --target latest-linux-x64,latest-win-x64,latest-mac-x64",
    "generate-docs": "./node_modules/.bin/typedoc --out docs ./src/ --options ./typedoc.json &>/dev/null",
    "watch": "nodemon --watch src/ --exec 'npm run compile && npm start' -e ts",
    "commit": "git cz",
    "lint": "eslint .",
    "lint:fix": "eslint --fix ."
  },
  "bin": "bin.js",
  "author": "OSTEP",
  "contributors": [
    "Miguel Roncancio (https://github.com/miggs125)",
    "Pouya Oftadeh (https://github.com/poftadeh)"
  ],
  "license": "MIT",
  "pkg": {
    "assets": [
      "./node_modules/vm2",
      "./node_modules/@types",
      "./node_modules/typescript/lib/*.d.ts",
      "./src/**/*.ts",
      "./tsconfig.json"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
